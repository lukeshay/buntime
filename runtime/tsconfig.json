{
	"compilerOptions": {
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"downlevelIteration": true,
		"forceConsistentCasingInFileNames": true,
		"jsx": "preserve",
		"lib": ["ESNext"],
		"module": "esnext",
		"moduleResolution": "nodenext",
		"skipLibCheck": true,
		"strict": true,
		"target": "esnext",
		"types": ["bun-types"]
	}
}
